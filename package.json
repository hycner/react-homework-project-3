{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:dry": "prettier -l \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "antd": "^3.4.3",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "seamless-immutable": "7.1.3",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "flow-bin": "0.69.0",
    "prettier": "1.11.1",
    "react-scripts": "1.1.3"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
